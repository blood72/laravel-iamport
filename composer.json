{
    "name": "blood72/laravel-iamport",
    "type": "library",
    "description": "Iamport API for Laravel 7",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "blood72",
            "email": "blood72@naver.com"
        }
    ],
    "support": {
        "issues": "https://github.com/blood72/laravel-iamport/issues",
        "source": "https://github.com/blood72/laravel-iamport"
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "orchestra/testbench": "^5.3|^6.4"
    },
    "autoload": {
        "psr-4": {
            "Blood72\\Iamport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blood72\\Iamport\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blood72\\Iamport\\IamportServiceProvider"
            ],
            "aliases": {
                "Iamport": "Blood72\\Iamport\\Facades\\IamportFacade"
            }
        }
    },
    "scripts": {
        "format": "php-cs-fixer fix .",
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
